<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.pizzagui.ConfirmationController">
    <BorderPane layoutX="200.0" layoutY="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <top>
            <AnchorPane prefHeight="70.0" prefWidth="1200.0" style="-fx-background-color: #F4B06A#F4B06A;" BorderPane.alignment="CENTER">
                <Label fx:id="currentUser" layoutX="6.0" layoutY="6.0" text="Current User: Employee" textFill="WHITE">
                    <font>
                        <Font name="Arial Rounded MT Bold" size="29.0" />
                    </font>
                </Label>
                <Button layoutX="650.0" layoutY="6.0" mnemonicParsing="false" onAction="#logOut" prefHeight="48.0" prefWidth="221.0" text="LogOut" textFill="#f4b06a">
                    <font>
                        <Font name="Arial Rounded MT Bold" size="29.0" />
                    </font>
                </Button>
                <Label fx:id="label" layoutX="14.0" layoutY="40.0" text="Confirmation" textAlignment="CENTER">
                    <font>
                        <Font name="Arial Rounded MT Bold" size="20.0" />
                    </font>
                </Label>
            </AnchorPane>
        </top>
        <center>
            <AnchorPane BorderPane.alignment="CENTER">
                <Button layoutX="32.0" layoutY="467.0" mnemonicParsing="false" onAction="#goBackToCheckout" prefHeight="49.0" prefWidth="112.0" style="-fx-background-color: #F4B06A#F4B06A #F4B06A#F4B06A;" text="Back" textFill="WHITE">
                    <font>
                        <Font name="Arial Rounded MT Bold" size="26.0" />
                    </font>
                </Button>
                <Button layoutX="252.0" layoutY="133.0" mnemonicParsing="false" prefHeight="48.0" prefWidth="409.0" style="-fx-background-color: #F4B06A#F4B06A #F4B06A#F4B06A;" text="Update Customer Info" textFill="WHITE">
                    <font>
                        <Font name="Arial Rounded MT Bold" size="26.0" />
                    </font>
                </Button>
                <Label layoutX="144.0" layoutY="209.0" text="Start Order :">
                    <font>
                        <Font name="Arial Rounded MT Bold" size="30.0" />
                    </font>
                </Label>
                <Button layoutX="680.0" layoutY="467.0" mnemonicParsing="false" onAction="#submit" prefHeight="48.0" prefWidth="206.0" style="-fx-background-color: #F4B06A#F4B06A #F4B06A#F4B06A;" text="Submit" textFill="WHITE">
                    <font>
                        <Font name="Arial Rounded MT Bold" size="26.0" />
                    </font>
                </Button>
                <TextField fx:id="custPhoneNumber" layoutX="292.0" layoutY="253.0" />
                <Text layoutX="169.0" layoutY="270.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Customer Phone #: ">
                    <font>
                        <Font name="Arial" size="12.0" />
                    </font>
                </Text>
                <Label fx:id="failedText" layoutX="352.0" layoutY="388.0" prefHeight="17.0" prefWidth="215.0" textAlignment="CENTER">
                    <font>
                        <Font name="Arial Rounded MT Bold" size="14.0" />
                    </font>
                </Label>
                <RadioButton fx:id="pickUp" layoutX="474.0" layoutY="250.0" mnemonicParsing="false" text="Pick-Up" toggleGroup="$toggleOrderType">
                    <toggleGroup>
                        <ToggleGroup fx:id="toggleOrderType" />
                    </toggleGroup>
                </RadioButton>
                <RadioButton fx:id="delivery" layoutX="610.0" layoutY="250.0" mnemonicParsing="false" text="Delivery" toggleGroup="$toggleOrderType" />
                <Label fx:id="failedText1" layoutX="352.0" layoutY="417.0" prefHeight="17.0" prefWidth="215.0" textAlignment="CENTER">
                    <font>
                        <Font name="Arial Rounded MT Bold" size="14.0" />
                    </font>
                </Label>
            </AnchorPane>
        </center>
    </BorderPane>
</AnchorPane>
